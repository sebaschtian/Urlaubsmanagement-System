projection;
strict ( 2 );

define behavior for ZSP_C_MITARBEITER alias Mitarbeiter
{
  use create;
  use update;
  use delete;

  use association _Antrag { create; }
  use association _Urlaub;
}

define behavior for ZSP_C_ANTRAG alias Antrag
{
  use update;
  use delete;

  use association _Antragsteller;
  use association _Genehmigender;
}

define behavior for ZSP_C_URLAUB alias Urlaub
{


  use association _Mitarbeiter;
}